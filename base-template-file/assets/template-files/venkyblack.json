{
    "min_packer_version": "1.0.4",
    "variables": {
      "image_name": "{{env `IMAGE_NAME`}}"
    },
    "builders": [
    {
    "type": "googlecompute",
    "image_name": "{{user `image_name`}}",
    "image_family": "new-lowes-centos",
    "project_id": "spicy-carbon",
    "source_image_family": "centos-7",
    "zone": "us-east4-a",
    "subnetwork": "default",
    "ssh_username": "centos"
    }
    ],
    "provisioners": [
    {
    "type": "file",
    "source": "./base-template-file/assets/gossfolder",
    "destination": "/tmp"
    },
    {
    "type": "shell",
    "script": "./base-template-file/assets/template-files/scripts/install_backup.sh"
    }
    ]
}